<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/ARFragment">

    <fragment
        android:id="@+id/ARFragment"
        android:name="com.ninjaturtles.usetogether.ar_helper.ARActivity"
        android:label="fragment_a_r"
        tools:layout="@layout/activity_a_r" >
        <argument
            android:name="originLongitude"
            app:argType="float"
            android:defaultValue="0.0"/>

        <argument
            android:name="originLatitude"
            app:argType="float"
            android:defaultValue="0.0"/>

        <argument
            android:name="destinationLongitude"
            app:argType="float"
            android:defaultValue="0.0"/>

        <argument
            android:name="destinationLatitude"
            app:argType="float"
            android:defaultValue="0.0"/>

        <argument
            android:name="plate"
            app:argType="string"
            android:defaultValue="" />

    </fragment>
</navigation>